<%- include("../../partials/header") %>
    <main>
    
        <section class="py-5 text-center container">
            <img src="./images/eveningresultbanner.jpg" class="img-fluid" alt="...">
            <div class="p-4"></div>
            <%if(currentUser){%>
                <%if(currentUser.role.includes('admin')){%>
            <a href="/previouseveningteerresult/addeveningteerresult"  type="button" class="btn btn-sm mb-3  btn-dark">Add Evening Teer Result</a>
            <%}%>
                <%}%>
            <div class="row no-gutters">
    
                <table class="table table-dark">
    <h5 class="bg-dark text-white mb-0 p-3">Evening Result</h5>
                    <tbody>
                        <thead>
    
                            <th>City</th>
                            <th>Date & Time</th>
                            <th>First Round</th>
                            <th>Second Round</th>
                            <%if(currentUser){%>
                                <%if(currentUser.role.includes('admin')){%>
                            <th>Action</th>
                            <%}%>
                                <%}%>
                        </thead>
                        <%eveningteerresult.forEach((data)=>{%>
    
                            <tr>
    
                                <td><%=data.city%></td>
                                <td>
                                    <%=data.date%>
                                </td>
                                <td>
                                    <%=data.firstround%>
                                </td>
                                <td>
                                    <%=data.secondround%>
                                </td>
                                <%if(currentUser){%>
                                    <%if(currentUser.role.includes('admin')){%>
                                <td>
                                    <form action="/previouseveningteerresult/<%=data._id %>/delete?_method=DELETE" method="POST">
                                        <button class="btn btn-sm  btn-dark text-center bg-white text-dark mb-3" class="">Delete</button>
                                    </form>
                                </td>
                                    <%}%>
                                        <%}%>
                            </tr>
                            <%})%>
                    </tbody>
                </table>
            </div>
            </div>
            </div>
            </div>
            </div>
        </section>
    </main>
    <%- include("../../partials/footer") %>